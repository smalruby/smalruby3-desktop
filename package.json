{
  "name": "smalruby3-desktop",
  "productName": "Smalruby3 Desktop",
  "description": "Smalruby3 as a self-contained desktop application forked from [LLK/scratch-desktop](https://github.com/LLK/scratch-desktop)",
  "author": "Ruby Programming Shounendan",
  "version": "1.0.0",
  "license": "BSD-3-Clause",
  "scripts": {
    "start": "electron-webpack dev --bail --display-error-details --env.minify=false",
    "build-gui": "node ./scripts/run-in-gui.js build",
    "watch-gui": "node ./scripts/run-in-gui.js watch",
    "clean": "rimraf ./dist/ ./static/assets/",
    "compile": "rimraf ./dist/ && electron-webpack --bail --display-error-details --env.minify=false",
    "fetch": "rimraf ./static/assets/ && mkdirp ./static/assets/ && node ./scripts/fetchMediaLibraryAssets.js && npm run fetch:setup-opal",
    "fetch:setup-opal": "rimraf ./static/javascripts && mkdirp ./static/javascripts && cp-cli ./node_modules/smalruby3-gui/static/javascripts/setup-opal.js ./static/javascripts/setup-opal.js",
    "dist": "npm run build-gui && npm run fetch && npm run compile -p && node ./scripts/electron-builder-wrapper.js",
    "dist:dir": "npm run dist -- --dir -c.compression=store -c.mac.identity=null",
    "lint": "eslint --cache --color --ext .jsx,.js ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/smalruby/smalruby3-desktop.git"
  },
  "dependencies": {
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "async": "^2.6.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-intl": "^2.3.1",
    "copy-webpack-plugin": "^4.6.0",
    "cp-cli": "^2.0.0",
    "electron": "^6.1.8",
    "electron-builder": "^22.3.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-notarize": "^0.2.1",
    "electron-store": "^3.3.0",
    "electron-webpack": "^2.7.4",
    "eslint": "^5.16.0",
    "eslint-config-scratch": "^5.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.18.3",
    "intl": "1.2.5",
    "lodash.bindall": "^4.4.0",
    "lodash.defaultsdeep": "^4.6.1",
    "mkdirp": "^0.5.1",
    "nets": "^3.2.0",
    "react": "16.2.0",
    "react-dom": "16.2.1",
    "react-intl": "2.4.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "rimraf": "^2.7.1",
    "smalruby3-gui": "github:smalruby/smalruby3-gui#smalruby3-desktop-v1.0.0",
    "source-map-loader": "^0.2.4",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "uuid": "^3.4.0",
    "webpack": "^4.41.6"
  },
  "resolutions": {
    "upath": "^1.0.5"
  }
}
